{% extends "templates/sink.njk" %}
{% block body %}
    <h1 class="hostile-sink-reset">Lists Sink</h1>

    <div class="spirit-container">
        <div class="spirit-container__inner">

            <h2 class="hostile-sink-reset">List</h2>
            {% call library.list() %}
                {{ library.list_item() }}
                {{ library.list_item(title_text="This is a list item with a very long title to see how it looks in comparison") }}
                {{ library.list_item() }}
            {% endcall %}

            <h2 class="hostile-sink-reset">List Title Meta</h2>
            {% call library.list() %}
                {{ library.list_item(title_meta="$9999") }}
                {{ library.list_item(title_text="This is a list item with a very long title to see how it looks in comparison", title_meta="$9999") }}
                {{ library.list_item(title_meta="$9999") }}
            {% endcall %}

            <h2 class="hostile-sink-reset">List Ordered</h2>
            {% call library.list(ordered="true") %}
                {{ library.list_item(title_meta="$9999") }}
                {{ library.list_item() }}
                {{ library.list_item(title_text="This is a list item with a very long title to see how it looks in comparison", title_meta="$9999") }}
            {% endcall %}

            <h2 class="hostile-sink-reset">List Links</h2>
            {% call library.list() %}
                {{ library.list_item(href="#") }}
                {{ library.list_item(href="#", title_text="This is a list item with a very long title to see how it looks in comparison", href_text="Read More") }}
                {{ library.list_item(href="#", href_secondary="#", href_secondary_text="Click Here") }}
                {{ library.list_item(href="#", href_secondary="#", href_secondary_text="Click Here") }}
                {{ library.list_item(href="#", title_text="Link Title Hover", class="spirit-list-item--title-hover", href_secondary="#", href_secondary_text="Click Here") }}
                {{ library.list_item(href="#", title_text="Link Title Active / Focus", class="spirit-list-item--title-focus", href_secondary="#", href_secondary_text="Click Here") }}
                {{ library.list_item(href="#", title_text="Link Action Hover", class="spirit-list-item--href-hover", href_secondary="#", href_secondary_text="Click Here") }}
                {{ library.list_item(href="#", title_text="Link Action Active / Focus", class="spirit-list-item--href-focus", href_secondary="#", href_secondary_text="Click Here") }}
                {{ library.list_item(href="#", title_text="Link Action Secondary Hover", class="spirit-list-item--href-hover-secondary", href_secondary="#", href_secondary_text="Click Here") }}
                {{ library.list_item(href="#", title_text="Link Action Secondary Active / Focus", class="spirit-list-item--href-focus-secondary", href_secondary="#", href_secondary_text="Click Here") }}
                {{ library.list_item(href="#", links_right=true, href_text="Read More") }}
                {{ library.list_item(href="#", title_text="This is a list item with a very long title to see how it looks in comparison", links_right=true, href_secondary="#", href_secondary_text="Click Here") }}
                {{ library.list_item(href="#", links_right=true, href_secondary="#", href_secondary_text="Click Here") }}
                {{ library.list_item(title_meta="$9999", href="#", href_secondary="#") }}
                {{ library.list_item(title_meta="$9999", href="#", href_text="Read More", href_secondary="#", href_secondary_text="Click Here") }}
            {% endcall %}

            <h2 class="hostile-sink-reset">List Avatar</h2>
            {% call library.list() %}
                {{ library.list_item(title_meta="$9999", icon_name="user", href="#", href_text="Read More", href_secondary="#", href_secondary_text="Click Here") }}
                {{ library.list_item(icon_name="user", href="#", href_text="Read More") }}
                {{ library.list_item(icon_name="user", title_text="This is a list item with a very long title to see how it looks in comparison", href="#", href_text="Read More") }}
                {{ library.list_item(icon_name="user") }}
                {{ library.list_item(title_meta="$9999", href="#", href_text="Read More", href_secondary="#", href_secondary_text="Click Here", avatar_src="https://picsum.photos/100/300") }}
                {{ library.list_item(href="#", href_text="Read More", avatar_src="https://picsum.photos/300/300") }}
                {{ library.list_item(avatar_src="https://picsum.photos/300/100") }}
            {% endcall %}

            <h2 class="hostile-sink-reset">List Image</h2>
